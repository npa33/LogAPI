{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Debug",
        "System": "Warning"
      }
    }
  },
  "DatabaseSettings": {
    "DbProvider": "mongodb",
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "LogData"
  },
  "AllowedHosts": "*"
}
